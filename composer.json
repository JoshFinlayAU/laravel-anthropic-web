{
    "name": "joshfinlayau/laravel-anthropic-web",
    "description": "Laravel client for Anthropic's Claude API with web search and fetch capabilities",
    "keywords": ["laravel", "anthropic", "claude", "ai", "web-search", "web-fetch"],
    "license": "MIT",
    "type": "library",
    "authors": [
        {
            "name": "Josh Finlay",
            "email": "josh@athenanetworks.com.au"
        }
    ],
    "require": {
        "php": "^8.1",
        "illuminate/support": "^9.0|^10.0|^11.0|^12.0",
        "illuminate/http": "^9.0|^10.0|^11.0|^12.0",
        "guzzlehttp/guzzle": "^7.0"
    },
    "require-dev": {
        "orchestra/testbench": "^7.0|^8.0|^9.0",
        "phpunit/phpunit": "^9.0|^10.0|^11.0",
        "laravel/pint": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "JoshFinlayAU\\LaravelAnthropicWeb\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "JoshFinlayAU\\LaravelAnthropicWeb\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "JoshFinlayAU\\LaravelAnthropicWeb\\AnthropicWebServiceProvider"
            ],
            "aliases": {
                "AnthropicWeb": "JoshFinlayAU\\LaravelAnthropicWeb\\Facades\\AnthropicWeb"
            }
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
